{
  "name": "smarttemp",
  "version": "0.1.0",
  "description": "HtmlData for smartTemp",
  "main": "src/node/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "webpack-dev-server --mode=development --env.DEFINE_SMT_VERSION=${npm_package_version} --env.DEFINE_SMT_HOST=smarttemp --env.DEFINE_DEBUG_REMOTE=False",
    "compile": "webpack --mode=production --env.DEFINE_SMT_VERSION=${DEFINE_SMT_VERSION:-${npm_package_version}} --env.DEFINE_SMT_HOST=${DEFINE_SMT_HOST:-'SmartTemp' } --env.DEFINE_DEBUG_REMOTE=${DEFINE_DEBUG_REMOTE:-'True'}",
    "postinstall": "npm run compile",
    "clean": "rimraf data",
    "cleanall": "rimraf data node_modules"
  },
  "repository": {
